#!/usr/bin/env zsh

# GNS Shortcuts
alias lca1-gns01="ssh lca1-gns01"
alias lca1-gns02="ssh lca1-gns02"
alias lva1-gns01="ssh lva1-gns01"
alias lva1-gns02="ssh lva1-gns02"
alias ltx1-gns01="ssh ltx1-gns01"
alias ltx1-gns02="ssh ltx1-gns02"
alias ltx1-gnsa01="ssh ltx1-gnsa01"
alias idb2-gns01="ssh idb2-gns01"
alias idb2-gns02="ssh idb2-gns02"
alias nsg7-gns01="ssh nsg7-gns01"
alias nsg7-gns02="ssh nsg7-gns02"
alias nsg7-gnsa01="ssh nsg7-gnsa01"
alias nsg7-portalcn01="ssh nsg7-portalcn01"
alias pt="ssh ptongsak-ld1"

# Decrypt GPG file
alias gpg_decrypt="gpg -d ~/'OneDrive - Microsoft'/gns_pass.gpg"

# Console access
alias screen_airconsole="screen /dev/cu.Airconsole-1 9600"
alias screen_console="screen /dev/cu.usbserial-DN05LJKP 9600"

# GNSOps
alias gns-ans="cd ~/projects/lnkd/gns/gns-ansible && gl"

# GlobalProtect
alias gpro="~/'OneDrive - Microsoft'/scripts/gpro.sh"

# SSH IdentitiesOnly
alias ssho="ssh -o IdentitiesOnly=yes"

# SSH Legacy
alias sshl="ssh -o KexAlgorithms=+diffie-hellman-group1-sha1"
